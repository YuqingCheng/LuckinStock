<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuqingcheng.luckinstock">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".AddStockActivity" />
        <activity android:name=".LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManageBasketActivity" />
        <activity android:name=".SignupLoginActivity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".EditBasketActivity" />
        <activity android:name=".EditStockInBasketActivity" />
        <activity android:name=".StrategyAnalysisActivity" />
        <activity android:name=".EditStrategyActivity"></activity>
    </application>

</manifest>